{
  "name": "Quote Tweet Viewer",
  "description": "Adds a “see quote tweets” button to Twitter. Quote icon by Recep Kutuk.",
  "version": "0.1.0",
  "manifest_version": 2,
  "web_accessible_resources": [
    "fonts/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "tweet.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/quote_16.png",
    "32": "icons/quote_32.png",
    "128": "icons/quote_128.png"
  }
}
